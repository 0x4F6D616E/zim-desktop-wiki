#: zim/plugins/versioncontrol/__init__.py:40
msgid "S_ave Version..."
msgstr "Zapisane wersje..."

#. menu item
#: zim/plugins/versioncontrol/__init__.py:41
msgid "_Versions..."
msgstr "Wersje..."

#. verbose error description
#: zim/plugins/versioncontrol/__init__.py:47
msgid ""
"There are no changes in this notebook since the last version that was saved"
msgstr "Brak zmian od ostatniej zapisanej wersji"

#. Short error descriotion
#: zim/plugins/versioncontrol/__init__.py:50
msgid "No changes since last version"
msgstr "Brak zmian od ostatniej wersji"

#. plugin name
#: zim/plugins/versioncontrol/__init__.py:57
msgid "Version Control"
msgstr "Kontrola wersji"

#. plugin description
#: zim/plugins/versioncontrol/__init__.py:58
msgid ""
"This plugin adds version control for notebooks.\n"
"\n"
"This plugin is based on the Bazaar version control system.\n"
"\n"
"This is a core plugin shipping with zim.\n"
msgstr ""
"Wtyczka ta dodaje kontrolę wersji w notesach.\n"
"\n"
"Wtyczka ta jest oparta na systemie kontroli wersji Bazaar.\n"
"\n"
"Jest to wtyczka domyślnie dostarczana z programem Zim.\n"

#. Label for plugin preference
#: zim/plugins/versioncontrol/__init__.py:70
msgid "Autosave version on regular intervals"
msgstr "Automatyczny zapis wersji w określonych przedziałach czasu"

#. default version comment for auto-saved versions
#: zim/plugins/versioncontrol/__init__.py:137
msgid "Automatically saved version from zim"
msgstr "Automatycznie zapisana wersja z programu Zim."

#. Question dialog
#: zim/plugins/versioncontrol/__init__.py:146
msgid "Enable Version Control?"
msgstr "Włączyć kontrolę wersji?"

#. Detailed question
#: zim/plugins/versioncontrol/__init__.py:147
msgid ""
"Version control is currently not enabled for this notebook.\n"
"Do you want to enable it?"
msgstr ""
"Kontrola wersji nie jest włączona dla tego notesu.\n"
"Czy chcesz ją włączyć?"

#. dialog title
#: zim/plugins/versioncontrol/__init__.py:181
msgid "Save Version"
msgstr "Zapisz wersję"

#. Dialog text
#: zim/plugins/versioncontrol/__init__.py:185
msgid "Please enter a comment for this version"
msgstr "Wpisz komentarz do tej wersji"

#. default version comment in the "save version" dialog
#: zim/plugins/versioncontrol/__init__.py:190
msgid "Saved version from zim"
msgstr "Zapisana wersja z programu Zim."

#. section for version details in "save version" dialog
#: zim/plugins/versioncontrol/__init__.py:198
msgid "Details"
msgstr "Szczegóły"

#. Option in versions dialog to show version for single page
#: zim/plugins/versioncontrol/__init__.py:247
msgid "_Page"
msgstr "_Strona"

#. Button label
#: zim/plugins/versioncontrol/__init__.py:261
msgid "View _Annotated"
msgstr "Pokaż _opisane"

#. Help text in versions dialog
#: zim/plugins/versioncontrol/__init__.py:266
msgid ""
"Select a version to see changes between that version and the current\n"
"state. Or select multiple versions to see changes between those versions.\n"
msgstr ""
"Wybierz wersję, którą chcesz porównać z obecnym stanem.\n"
"Możesz też wybrać wiele wersji, by porównać je między sobą.\n"

#. version details
#: zim/plugins/versioncontrol/__init__.py:288
msgid "Comment"
msgstr "Komentarz"

#. Button label
#: zim/plugins/versioncontrol/__init__.py:304
msgid "_Restore Version"
msgstr "_Przywróć wersję"

#. button in versions dialog for diff
#: zim/plugins/versioncontrol/__init__.py:309
msgid "Show _Changes"
msgstr "Pokaż _Zmiany"

#. button in versions dialog for side by side comparison
#: zim/plugins/versioncontrol/__init__.py:315
msgid "_Side by Side"
msgstr "_Obok siebie"

#. dialog title
#: zim/plugins/versioncontrol/__init__.py:394
msgid "Annotated Page Source"
msgstr "Opisane źródło strony"

#. Confirmation question
#: zim/plugins/versioncontrol/__init__.py:403
msgid "Restore page to saved version?"
msgstr "Przywrócić stronę do zapisanej wersji?"

#. Detailed question, "%(page)s" is replaced by the page, "%(version)s" by the version id
#: zim/plugins/versioncontrol/__init__.py:404
#, python-format
msgid ""
"Do you want to restore page: %(page)s\n"
"to saved version: %(version)s ?\n"
"\n"
"All changes since the last saved version will be lost !"
msgstr ""
"Czy chcesz przywrócić stronę: %(page)s\n"
"do zapisanej wersji: %(version)s ?\n"
"\n"
"Wszystkie zmiany wprowadzone po zapisaniu ostatniej wersji zostaną utracone!"

#. dialog title
#: zim/plugins/versioncontrol/__init__.py:418
msgid "Changes"
msgstr "Zmiany"

