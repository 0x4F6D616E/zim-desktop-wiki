<ui>
	<menubar name='menubar'>
		<menu action='file_menu'>
			<menuitem action='new_page'/>
			<menuitem action='open_notebook'/>
			<separator/>
			<menuitem action='save_page'/>
			<menuitem action='save_version'/>
			<menuitem action='show_versions'/>
			<separator/>
			<menuitem action='show_export'/>
			<placeholder name='print_actions'/>
			<menuitem action='email_page'/>
			<separator/>
			<placeholder name='page_mods'/>
			<separator/>
			<menuitem action='show_properties'/>
			<separator/>
			<menuitem action='close'/>
			<menuitem action='quit'/>
		</menu>
		<menu action='edit_menu'>
			<placeholder name='edit_page'/>
			<separator/>
			<menuitem action='copy_location'/>
			<separator/>
			<menuitem action='show_preferences'/>
		</menu>
		<menu action='view_menu'>
			<menuitem action='toggle_toolbar'/>
			<menuitem action='toggle_statusbar'/>
			<menuitem action='toggle_sidepane'/>
			<menu action='path_bar_menu'>
				<menuitem action='set_pathbar_recent'/>
				<menuitem action='set_pathbar_history'/>
				<menuitem action='set_pathbar_namespace'/>
				<menuitem action='set_pathbar_hidden'/>
			</menu>
			<separator/>
			<placeholder name='plugin_items'/>
			<separator/>
			<menuitem action='reload_page'/>
		</menu>
		<menu action='insert_menu'>
		</menu>
		<menu action='format_menu'></menu>
		<menu action='search_menu'>
			<placeholder name='find_items'/>
			<separator/>
			<menuitem action='show_search'/>
			<menuitem action='show_search_backlinks'/>
		</menu>
		<menu action='tools_menu'>
			<placeholder name='page_tools'/>
			<separator/>
			<menuitem action='edit_page_source'/>
			<menuitem action='attach_file'/>
			<menuitem action='open_attachments_folder'/>
			<menuitem action='open_documents_folder'/>
			<separator/>
			<menuitem action='show_server_gui'/>
			<separator/>
			<menuitem action='reload_index'/>
		</menu>
		<placeholder name='plugin_menus'/>
		<menu action='go_menu'>
			<menuitem action='go_page_back'/>
			<menuitem action='go_page_forward'/>
			<separator/>
			<menuitem action='go_page_parent'/>
			<menuitem action='go_page_child'/>
			<menuitem action='go_page_next'/>
			<menuitem action='go_page_prev'/>
			<separator/>
			<placeholder name='plugin_items'/>
			<separator/>
			<menuitem action='go_page_home'/>
			<menuitem action='open_page'/>
		</menu>
		<menu action='help_menu'>
			<menuitem action='show_help'/>
			<menuitem action='show_help_faq'/>
			<menuitem action='show_help_keys'/>
			<menuitem action='show_help_bugs'/>
			<separator/>
			<menuitem action='show_about'/>
		</menu>
	</menubar>
	<toolbar name='toolbar'>
		<placeholder name='file'/>
		<separator/>
		<placeholder name='edit'/>
		<separator/>
		<placeholder name='view'>
			<toolitem action='toggle_sidepane'/>
		</placeholder>
		<separator/>
		<placeholder name='go'>
			<toolitem action='go_page_home'/>
			<toolitem action='go_page_back'/>
			<toolitem action='go_page_forward'/>
		</placeholder>
		<separator/>
		<placeholder name='search'/>
		<separator/>
		<placeholder name='format'/>
		<separator/>
		<placeholder name='tools'>
			<toolitem action='attach_file'/>
		</placeholder>
	</toolbar>
